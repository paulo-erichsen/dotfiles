# switch between the 7.3.1 and latest versions of xcode
# this function assumes you have v7.3.1 installed in /Applications/Xcode-7.3.1.app
function switch_xcode {
    [ -x /usr/bin/xcode-select ] || return
    if [ $(xcode-select -p) == "/Applications/Xcode.app/Contents/Developer" ]; then
        sudo xcode-select --switch /Applications/Xcode-7.3.1.app
    else
        sudo xcode-select --switch /Applications/Xcode.app
    fi
}
