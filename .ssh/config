# ~/.ssh/config

### default for all 10.75.* ###
Host 10.75.*
    Port 22
    User root
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null

Host esm
     Hostname 10.75.96.130

Host esm1
     Hostname 10.75.96.131

Host esm2
     Hostname 10.75.96.132

Host esm3
     Hostname 10.75.96.133

Host esm4
     Hostname 10.75.96.134

# keep amazon aws from timing out
Host aws
     Hostname 52.88.162.80
     User ec2-user
     ServerAliveInterval 50
     IdentityFile ~/.ssh/paulo.pem
