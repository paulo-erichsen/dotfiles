Include ~/.ssh/config.d/*

Host nas
     HostName 10.10.10.2
     User admin
     ServerAliveInterval 10
     ServerAliveCountMax 30

Host odroid
     HostName 10.10.1.3
     User paulo

Host rpizero
     HostName 10.10.1.4
     User pi

Host pdu
     HostName 10.10.1.141
     User pdu_usern4me
     KexAlgorithms +diffie-hellman-group-exchange-sha1
     Ciphers aes128-cbc

Host qemu
     HostName 192.168.7.2
     User root
     StrictHostKeyChecking no
     UserKnownHostsFile /dev/null

Host vagrant
     HostName localhost
     User vagrant
     Port 3022
     StrictHostKeyChecking no
     UserKnownHostsFile /dev/null
