Include ~/.ssh/config.d/*

Host nas
     HostName 10.10.1.2
     User admin

Host qemu
     HostName 192.168.7.2
     User root
     StrictHostKeyChecking no
     UserKnownHostsFile /dev/null

Host vagrant
     HostName localhost
     User vagrant
     Port 3022
     StrictHostKeyChecking no
     UserKnownHostsFile /dev/null
